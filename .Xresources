XTerm*locale: true
XTerm*faceName: xft:Meslo LG S for Powerline:size=10
XTerm*faceNameDoublesize: IPAMincho:pixelsize=12
XTerm*selectToClipboard: true
XTerm*utf8: true
XTerm*utf8Title: true
XTerm*inputMethod: ibus

*VT100*eightBitInput: false
*VT100*eightBitControl: false
*VT100*eightBitOutput: true

Xft.antialias: true

! kantan Xresources template
*.foreground: #d0d0d0
*.background: #151515
*.cursorColor: #d0d0d0
 
! Black / Grey
